{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ],
      "urls": [
        "http://fonts.googleapis.com/**",
        "https://fonts.googleapis.com/css?family=Roboto+Slab",
        "http://api.openweathermap.org/data/2.5/",
        "https://ipinfo.io/json",
        "http://localhost:8080/**",
        "https://weather-app-44d0d.firebaseapp.com/**"
      ]
    }
   }],
   "dataGroups": [
     {
      "name": "search-weather-api",
       "urls": ["/services/location", "/services/weather"],
       "cacheConfig": {
         "strategy": "freshness",
         "maxSize": 20,
         "maxAge": "1h",
         "timeout": "5s"
       }

     }
   ]
}
